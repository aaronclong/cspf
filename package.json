{
  "name": "cspf",
  "version": "0.0.1",
  "description": "CBOR XSPF JavaScript module. This is an unofficial fork of XSPF that implements CBOR & DAR-CBOR compliant version",
  "keywords": [
    "xspf",
    "jspf",
    "cspf",
    "playlist"
  ],
  "homepage": "https://github.com/aaronclong/cspf",
  "bugs": {
    "url": "https://github.com/aaronclong/cspf/issues"
  },
  "author": "Aaron Long",
  "engines": {
    "node": ">=24.11.1"
  },
  "dependencies": {
    "@ipld/dag-cbor": "^9.2.5"
  },
  "main": "dist/cspf.node.cjs",
  "module": "dist/cspf.browser.js",
  "types": "dist/types/index.d.ts",
  "browser": "dist/cspf.browser.js",
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/cspf.browser.js",
      "require": "./dist/cspf.node.cjs"
    }
  },
  "scripts": {
    "build": "rimraf dist && rollup -c"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/aaronclong/cspf.git"
  },
  "volta": {
    "node": "24.11.1",
    "pnpm": "10.24.0"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^29.0.0",
    "@rollup/plugin-node-resolve": "^16.0.3",
    "@rollup/plugin-typescript": "^12.3.0",
    "rimraf": "^6.1.2",
    "rollup": "^4.53.3",
    "tslib": "^2.8.1",
    "typescript": "^5.9.3",
    "vitest": "^4.0.15"
  }
}
