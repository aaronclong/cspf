{
  "compilerOptions": {
    "target": "ES2019",
    "module": "ESNext",
    "moduleResolution": "node",
    "lib": ["ES2019", "DOM"],
    "declaration": false,
    "emitDeclarationOnly": false,
    "sourceMap": true,
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "rootDir": ".",
    "outDir": "dist"
  },
  "include": ["cspf/index.ts"],
  "exclude": ["node_modules", "dist", "*.test.*"]
}
